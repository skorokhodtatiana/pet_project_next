generator client {
	provider = "prisma-client"
	output = "../src/generated/prisma"
}

datasource db {
	provider = "postgresql"
}

model User {
	id String @id @default(uuid())
	email String @unique
	password String

	createAt DateTime @default(now()) @map("created_at")
	updatedAt DateTime @updatedAt @map("updated_at")

	@@map("users")
}

